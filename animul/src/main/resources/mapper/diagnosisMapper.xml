<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="diagnosis">
	<insert id="insert" parameterType="diagnosisVO">
		insert into animul.receipt values (
			#{diagnosis_name},
			#{diagnosis_category}
		)
	</insert>
	
	<select id="one" parameterType="diagnosisVO" resultType="diagnosisVO">
		select * from animul.receipt_item where diagnosis_name = #{diagnosis_name}
	</select>
	<select id="list" resultType="diagnosisVO">
		select * from animul.receipt_item order by diagnosis_name desc
	</select>
</mapper>